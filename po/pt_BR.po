msgstr "Bem vindo a ajuda do %s Ncurses!\n"

#: tui_ncurses.c:479
msgid "This help describes how to use this Text-based User Interface."
msgstr ""
"Essa ajuda descreve como usar a interface do usuário com base em texto."

#: tui_ncurses.c:481
msgid "Global keys:"
msgstr "Chaves globais:"

#: tui_ncurses.c:482
msgid "Press 'left' key to switch in left tab."
msgstr "Pressione a tecla 'esquerda' para alternar para a aba esquerda."

#: tui_ncurses.c:483
msgid "Press 'right' key to switch in right tab."
msgstr "Pressione a tecla 'direita' para alternar para a aba direita."

#: tui_ncurses.c:484
msgid "Press 'h' key to see this help."
msgstr "Pressione 'h' para ver esta ajuda."

#: tui_ncurses.c:485
msgid "Press 'q' key to exit."
msgstr "Pressione a tecla 'q' para encerrar."

#: tui_ncurses.c:487
msgid "CPU tab:"
msgstr "Aba da CPU:"

#: tui_ncurses.c:488
msgid "Press 'down' key to decrease core number to monitor."
msgstr ""
"Pressione a tecla 'abaixo' para reduzir a quantidade dos núcleos que serão "
"monitorados."

#: tui_ncurses.c:489
msgid "Press 'up' key to increase core number to monitor."
msgstr ""
"Pressione a tecla 'acima' para aumentar a quantidade dos núcleos que serão "
"monitorados."

#: tui_ncurses.c:491
msgid "Caches tab:"
msgstr "Aba Caches:"

#: tui_ncurses.c:492
msgid "Press 'down' key to switch to previous test."
msgstr "Pressione a tecla 'abaixo' para alternar para o teste anterior."

#: tui_ncurses.c:493
msgid "Press 'up' key' to switch to next test."
msgstr "Pressione a tecla 'acima' para alternar para o próximo teste."

#: tui_ncurses.c:495
msgid "Bench tab:"
msgstr "Aba Bench:"

#: tui_ncurses.c:496
msgid "Press 'down' key to decrement benchmark duration."
msgstr "Pressione a tecla 'abaixo' para reduzir a duração do benchmark."

#: tui_ncurses.c:497
msgid "Press 'up' key to increment benchmark duration."
msgstr "Pressione a tecla 'acima' para aumentar a duração do benchmark."

#: tui_ncurses.c:498
msgid "Press 'next page' key to decrement number of threads to use."
msgstr ""
"Pressione a tecla 'próxima página' para reduzir a quantidade de threads."

#: tui_ncurses.c:499
msgid "Press 'previous page' key to increment number of threads to use."
msgstr ""
"Pressione a tecla 'página anterior' para aumentar a quantidade das threads."

#: tui_ncurses.c:500
msgid "Press 's' key to start/stop prime numbers (slow) benchmark."
msgstr ""
"Pressione a tecla 's' para iniciar/parar o benchmark dos números primos "
"(lento)."

#: tui_ncurses.c:501
msgid "Press 'f' key to start/stop prime numbers (fast) benchmark."
msgstr ""
"Pressione a tecla 'f' para iniciar/parar o benchmark dos números primos "
"(rápido)."

#: tui_ncurses.c:503
msgid "Graphics tab:"
msgstr "Aba gráficos:"

#: tui_ncurses.c:504
msgid "Press 'down' key to switch to previous graphic card."
msgstr ""
"Pressione a tecla 'abaixo' para alternar para a placa gráfica anterior."

#: tui_ncurses.c:505
msgid "Press 'up' key to switch to next graphic card."
msgstr "Pressione a tecla 'acima' para alternar para a próxima placa gráfica."

#: tui_ncurses.c:507
msgid "Press any key to exit this help."
msgstr "Pressione qualquer tecla para encerrar esta ajuda."

